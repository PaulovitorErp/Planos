		oSection1:Cell("VALOR_ABERTO"):SetValue(TRBU00->VALOR_ABERTO)

		oSection1:PrintLine()
		oReport:SkipLine()

		TRBU00->(dbSkip())
	EndDo

	oSection1:Finish()

	TRBU00->(dbCloseArea())

Return

