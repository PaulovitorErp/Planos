	TRFunction():New(oSection1:Cell("TIT_FATURADO"),/* cID */,"SUM",oBreak1,"Tit Faturados",/*cPicture*/,/*uFormula*/,.T. /*lEndSection*/,.F. /*lEndReport*/,/*lEndPage*/)
